{
    "name": "jonasof/laravel-route-debugger",
    "description": "Get file and line of matched laravel route",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": ">=7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4"
    },
    "autoload": {
        "psr-4": {
            "LaravelRouteFinder\\": "src/"
        }
    },
    "bin": [
        "bin/laravel-route-debugger"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "LaravelRouteFinder\\Console\\ServiceProvider"
            ]
        }
    }
}
